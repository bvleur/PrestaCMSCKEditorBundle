{
    "name": "presta/cms-ckeditor-bundle",
    "type": "symfony-bundle",
    "description": "PrestaCMS CKEditor bundle : Integrates CKEditor in PrestaCMS with SonataMedia and page links",
    "keywords": ["symfony2", "bundle", "cms", "symfony-cmf", "prestaconcept", "wysiwyg", "ckeditor"],
    "homepage": "https://github.com/prestaconcept/PrestaCMSCKEditorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bastien",
            "email": "nbastien@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        },
        {
            "name": "Nicolas Joubert",
            "email": "njoubert@prestaconcept.net",
            "role": "Maintainer, Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "tilleuls/ckeditor-sonata-media-bundle": "2.3.*",
        "presta/composer-public-bundle": "1.*",
        "ckeditor/ckeditor": ">=4.3.1"
    },
    "suggest": {
        "presta/cms-core-bundle": "1.0.*"
    },
    "autoload": {
        "psr-0": { "Presta\\CMSCKEditorBundle": "" }
    },
    "target-dir": "Presta/CMSCKEditorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
